{
    "manifest_version": 3,
    "name": "Toolbox fo dev",
    "description": "A toolbox for developers with many tools",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "src/html/popup.html",
      "default_icon": {
        "16": "src/img/tool.png",
        "48": "src/img/tool.png",
        "128": "src/img/tool.png"
      }
    },
    "icons": {
      "16": "src/img/tool.png",
      "48": "src/img/tool.png",
      "128": "src/img/tool.png"
    }
  }
  